  script_set_attribute(attribute:"plugin_type", value:"local");
  script_set_attribute(attribute:"cpe", value:"p-cpe:/a:novell:suse_linux:libgsf-1-114");
  script_set_attribute(attribute:"cpe", value:"p-cpe:/a:novell:suse_linux:libgsf-1-114-32bit");
  script_set_attribute(attribute:"cpe", value:"p-cpe:/a:novell:suse_linux:libgsf-lang");
